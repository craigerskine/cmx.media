        <b class="text-(pri-500 xs) leading-6 font-semibold flex-none">{{ sub }}</b>
      {% endif %}
      </figcaption>
    </figure>
    {%- if url !== '' %}</a>{%- endif %}
  </li>
{%- endmacro -%}