---
layout: layouts/default.njk
title: Misc
eleventyComputed:
  eleventyNavigation:
    key: "{{ title }}"
    ico: mdi:draw
    order: 3
---

{%- from 'macro.list.njk' import list, listItem -%}

{%- call list() %}
  {%- for type, items in collections.misc | groupby('data.title') %}
    {{ listItem({
      series: type,
      title: type,
      badge: items.length,
      svg: true,
      url: '/misc/'+ type | slugify +'/'
    }) }}
  {%- endfor %}
{%- endcall %}
