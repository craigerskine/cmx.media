---
layout: 'layouts/default.njk'
title: Comics
eleventyComputed:
  eleventyNavigation:
    key: "{{ title }}"
    ico: mdi:bookshelf
    order: 1
---

{%- import 'components/el.njk' as el -%}

{%- call el.list() %}
{%- for type, items in collections.comics | groupby('data.title') %}
  {{ el.listitem(title=type,sub=items.length,image='/_assets/img/comics/'+ type | slugify +'/series.jpg',url='/comics/'+ type | slugify +'/') }}
{%- endfor %}
{%- endcall %}